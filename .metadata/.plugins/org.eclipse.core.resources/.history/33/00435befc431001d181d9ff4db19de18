package practice;

public class Java_Interview_Programs {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//Fibonacci series
		int a = 0;
		int b = 1;
		int c = 0;
		for (int i = 0; c < 100; i++) {
			System.out.println(c);
			c = b + a;

			b = a;
			a = c;
		}

	}

}
